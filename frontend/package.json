{
    "name": "rabbit-frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build --mode=development",
        "build-watch": "vue-cli-service build --mode=development --watch",
        "build-prod": "vue-cli-service build",
        "test": "jest --updateSnapshot",
        "test:unit": "jest --updateSnapshot",
        "test:unit:ui": "majestic",
        "test:unit:ci": "jest --ci",
        "test:unit:coverage": "jest --coverage",
        "test:unit:watch": "jest --watch",
        "test:unit:watchAll": "jest --watchAll",
        "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
        "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
        "lint": "vue-cli-service lint",
        "lint-fix": "vue-cli-service lint --fix"
    },
    "dependencies": {
        "@quasar/extras": "^1.0.0",
        "@three-ts/orbit-controls": "^1.4.7",
        "ammo.js": "github:kripken/ammo.js",
        "axios": "^0.21.1",
        "core-js": "^3.6.5",
        "mosha-vue-toastify": "^1.0.21",
        "quasar": "^2.6.0",
        "three": "^0.132.2",
        "vue": "^3.2.13",
        "vue-class-component": "^8.0.0-0"
    },
    "devDependencies": {
        "@quasar/app-webpack": "^3.0.0",
        "@quasar/quasar-app-extension-testing": "^2.0.3",
        "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.10",
        "@types/jest": "^27.4.1",
        "@types/node": "^12.20.21",
        "@types/three": "^0.131.0",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "@vue/cli-plugin-babel": "~5.0.0",
        "@vue/cli-plugin-eslint": "~5.0.0",
        "@vue/cli-plugin-typescript": "~5.0.0",
        "@vue/cli-service": "~5.0.0",
        "@vue/compiler-sfc": "^3.0.0",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/eslint-config-typescript": "^9.1.0",
        "@vue/test-utils": "^2.0.0-0",
        "eslint": "^8.10.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-jest": "^25.2.2",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-vue": "^8.5.0",
        "majestic": "^1.7.0",
        "prettier": "^2.5.1",
        "sass": "^1.32.7",
        "sass-loader": "^12.0.0",
        "vue-cli-plugin-quasar": "~4.0.4",
        "vue-tsc": "^0.29.8"
    },
    "engines": {
        "node": ">= 12.22.1",
        "npm": ">= 6.13.4",
        "yarn": ">= 1.21.1"
    }
}
